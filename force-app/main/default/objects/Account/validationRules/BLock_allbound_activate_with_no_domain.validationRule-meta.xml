<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BLock_allbound_activate_with_no_domain</fullName>
    <active>true</active>
    <errorConditionFormula>and(
 TEXT(Allbound_Status__c) = &quot;Active&quot;,
 ISBLANK(Website )
)</errorConditionFormula>
    <errorDisplayField>Allbound_Status__c</errorDisplayField>
    <errorMessage>Please confirm a correct account website is entered BEFORE making this partner &quot;Allbound Active&quot;</errorMessage>
</ValidationRule>
